						<% } %>
					</div>
                </div>
				<div class="card card-block">
					<div class="col-sm-12">
						<div class="col-sm-2"></div>
							<div class="col-sm-8">
								<div class="card card-block">
									<img class="w-100" src="/<%-impact.image%>" alt="Achieving flexibility">
								</div>
							</div>
						<div class="col-sm-2"></div>
					</div>
				</div>
            </div>
        </div>
    </div>
	
	<div class="tab-pane" id="ppbg" role="tabpanel">
