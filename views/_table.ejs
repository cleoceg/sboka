<div class="row">
	<div class="col-sm-12">
		<div class="card card-block">
			<div class="card card-block">
				<table class="table table-hover">
					<thead>
						<tr>
							<% for (var i=0; i<data.nbcol; i++) { %>
								<th><%-data.head[i]%></th>
							<% } %>
						</tr>
					</thead>
					<tbody>
						<% for (var i=0; i<data.body.length; i++) {
							var row = data.body[i]; %>
							<tr>
								<% for (var j=0; j<data.nbcol; j++) { 
									var col = row[j]; %>
									<td>
										<% for (var k=0; k<col.length; k++) { %>
											<li><%-col[k]%>
										<% } %>
									</td>
								<% } %>	
							</tr>
						<% } %>
					</tbody>
				</table>
            </div>
        </div>
	</div>
</div>