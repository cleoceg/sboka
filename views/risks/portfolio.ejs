<%- include ../_header %>

<h1 class="display-4"><%=title%></h1>

<div class="col-sm-12">    
    <div class="card card-block">
        <% for (var i=0; i<intro.length; i++) { %>
            <p class="card-text"><%-intro[i]%>
        <% } %>
    </div>  
    <div class="card card-block">
        <% for (var i=0; i<inportfolio.length; i++) { %>
            <p class="card-text"><%-inportfolio[i]%>
        <% } %>
    </div>
    <div class="card card-block">
        <% for (var i=0; i<inprogram.length; i++) { %>
            <p class="card-text"><%-inprogram[i]%>
        <% } %>
    </div> 
    <div class="col-sm-12">
        <div class="col-sm-2"></div>
        <div class="col-sm-8">
            <div class="card card-block">
                <img class="w-100" src="/<%-image%>" alt="">
            </div>
        </div>
        <div class="col-sm-2"></div>
    </div>
</div>

<%- include ../_footer %>

